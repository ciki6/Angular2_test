{
  "name": "angular2-test",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "start": "npm run server:dev",
    "e2e": "protractor protractor.config.js",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "pree2e": "webdriver-manager update",
    "test": "karma start karma.conf.js",
    "test-once": "karma start karma.conf.js --single-run",
    "build:dev": "npm run clean:dist && webpack --config config/webpack.dev.js --progress --profile",
    "build:aot": "npm run clean:dist && webpack --config config/webpack.prod.aot.js --progress --profile --bail",
    "build:prod": "npm run clean:dist && webpack --config config/webpack.prod.js  --progress --profile --bail",
    "server:dev": "webpack-dev-server --config config/webpack.dev.js --progress --profile --watch --content-base app/",
    "server": "http-server dist --cors",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "webdriver:update": "npm run webdriver-manager update",
    "clean:dist": "npm run rimraf -- dist",
    "rimraf": "rimraf"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~2.4.0",
    "@angular/compiler": "~2.4.0",
    "@angular/compiler-cli": "^2.4.1",
    "@angular/core": "~2.4.0",
    "@angular/forms": "~2.4.0",
    "@angular/http": "~2.4.0",
    "@angular/platform-browser": "~2.4.0",
    "@angular/platform-browser-dynamic": "~2.4.0",
    "@angular/platform-server": "^2.4.1",
    "@angular/router": "~3.4.0",
    "@angularclass/conventions-loader": "^1.0.2",
    "angular-in-memory-web-api": "~0.2.1",
    "core-js": "^2.4.1",
    "immutable": "^3.8.1",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.2",
    "ts-helpers": "^1.1.2",
    "zone.js": "^0.7.4",
    "angular2-websocket": "^0.9.0"
  },
  "devDependencies": {
    "@ngtools/webpack": "^1.2.1",
    "@types/jasmine": "^2.5.41",
    "@types/node": "^6.0.55",
    "@types/selenium-webdriver": "^2.53.33",
    "@types/source-map": "^0.5.0",
    "@types/uglify-js": "^2.6.28",
    "@types/webpack": "^2.1.0",
    "angular2-router-loader": "^0.3.4",
    "angular2-template-loader": "^0.6.0",
    "assets-webpack-plugin": "^3.5.0",
    "awesome-typescript-loader": "^3.0.0-beta.17",
    "canonical-path": "0.0.2",
    "concurrently": "^3.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "http-server": "^0.9.0",
    "istanbul-instrumenter-loader": "^1.2.0",
    "jasmine-core": "~2.5.2",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.2.1",
    "karma-remap-coverage": "^0.1.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.1",
    "lodash": "^4.16.4",
    "protractor": "^5.1.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "script-ext-html-webpack-plugin": "^1.3.4",
    "source-map-explorer": "^1.3.3",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "to-string-loader": "^1.1.5",
    "ts-node": "^2.0.0",
    "tslint": "^4.2.0",
    "typescript": "^2.1.6",
    "url-loader": "^0.5.7",
    "v8-lazy-parse-webpack-plugin": "^0.3.0",
    "webpack": "2.2.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "2.1.0-beta.11",
    "webpack-md5-hash": "^0.0.5",
    "webpack-merge": "~2.0.0",
    "angular2-swiper-wrapper": "^2.2.2"
  }
}
